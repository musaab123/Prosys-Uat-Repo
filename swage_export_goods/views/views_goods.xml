<?xml version="1.0" encoding="utf-8"?>

<odoo>
  <data>
 
    <record id="view_stock_picking_form_inehrit" model="ir.ui.view">
            <field name="name">stock.picking.inherit</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='origin']" position="after">
                    <field name="custom_team_id"  invisible="1"/>

                </xpath>


            </field>
        </record>


    <record id="stock_picking_inherit_form" model="ir.ui.view">
        <field name="name">stock.picking.inherit.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="custom_team_id" />
            </xpath>
        </field>
    </record>


  

  </data>
</odoo>