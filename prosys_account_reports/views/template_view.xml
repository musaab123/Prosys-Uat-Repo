<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="search_template_sales_team_123">
            <button t-if="options.get('salesteam')" type="button" class="o-no-caret btn btn-secondary dropdown-toggle account_partner_filter" data-bs-toggle="dropdown">
                <span class="fa fa-folder-open"/>  Sales Team
            </button>
            <ul class="dropdown-menu o_filter_menu" role="menu">
                <li class="o_account_report_search js_sales_team_m2m"/>
            </ul>
        </template>

    
        <template id="prosys_search_template_1234" inherit_id="account_reports.search_template">
            <xpath expr="//div[hasclass('o_account_reports_filter_partner')]" position="after">
                <div class="btn-group dropdown o_account_reports_filter_sales_team" t-if="options.get('salesteam')">
                    <t t-call="prosys_account_reports.search_template_sales_team_123"/>
                </div>
            </xpath>
        </template>
    </data>
</odoo>
